@{
    ViewData["Title"] = "Home Page";
}

@model List<Country>

<form action="DisplayCountry" name="" id="countrySelect">
    <select for="Country" name="Country" form="countrySelect">
        @foreach (Country c in Model)
        {
            <option value="@c.Name">@c.Name</option>
        }
    </select><br />
    <input type="submit" value="Get Learnin'" />
</form>

